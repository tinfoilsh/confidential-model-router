{
  "dashboard": {
    "title": "Confidential Model Router - Overload & Performance Metrics",
    "description": "Monitoring dashboard for model router rate limiting and backend overload metrics",
    "timezone": "browser",
    "schemaVersion": 38,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Requests Rejected (Rate)",
        "type": "timeseries",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(router_requests_rejected_total[5m])",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10,
              "pointSize": 5
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 2,
        "title": "Total Requests Rejected",
        "type": "stat",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(router_requests_rejected_total)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "green"
                },
                {
                  "value": 100,
                  "color": "yellow"
                },
                {
                  "value": 1000,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Currently Overloaded Backends",
        "type": "stat",
        "gridPos": {
          "x": 18,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(router_backend_overloaded)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "green"
                },
                {
                  "value": 1,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Overload Events (Rate)",
        "type": "stat",
        "gridPos": {
          "x": 12,
          "y": 4,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(rate(router_overload_events_total[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "decimals": 2,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "green"
                },
                {
                  "value": 0.01,
                  "color": "yellow"
                },
                {
                  "value": 0.1,
                  "color": "red"
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Recovery Events (Rate)",
        "type": "stat",
        "gridPos": {
          "x": 18,
          "y": 4,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(rate(router_recovery_events_total[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "decimals": 2,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 0.01,
                  "color": "green"
                }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Backend Queue Depth by Model & Enclave",
        "type": "timeseries",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "router_backend_queue_depth",
            "legendFormat": "{{model}} - {{enclave}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 20,
              "pointSize": 5
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max", "last"]
          }
        }
      },
      {
        "id": 7,
        "title": "Backend Overload Status",
        "type": "timeseries",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "router_backend_overloaded",
            "legendFormat": "{{model}} - {{enclave}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "stepAfter",
              "fillOpacity": 50,
              "pointSize": 5
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byValue",
                "options": {
                  "value": 1
                }
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "red"
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 8,
        "title": "Retry-After Duration Distribution",
        "type": "timeseries",
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(router_retry_after_seconds_sum[5m]) / rate(router_retry_after_seconds_count[5m])",
            "legendFormat": "{{model}} - avg",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(router_retry_after_seconds_bucket[5m]))",
            "legendFormat": "{{model}} - p95",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10,
              "pointSize": 5
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max", "last"]
          }
        }
      },
      {
        "id": 9,
        "title": "Overload & Recovery Events by Model",
        "type": "timeseries",
        "gridPos": {
          "x": 12,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(router_overload_events_total[5m])",
            "legendFormat": "{{model}}-{{enclave}} overload",
            "refId": "A"
          },
          {
            "expr": "rate(router_recovery_events_total[5m])",
            "legendFormat": "{{model}}-{{enclave}} recovery",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "bars",
              "fillOpacity": 80
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 10,
        "title": "Requests Rejected by Model",
        "type": "piechart",
        "gridPos": {
          "x": 0,
          "y": 24,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum by (model) (router_requests_rejected_total)",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          },
          "pieType": "donut"
        }
      },
      {
        "id": 11,
        "title": "Average Queue Depth by Model",
        "type": "bargauge",
        "gridPos": {
          "x": 12,
          "y": 24,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "avg by (model) (router_backend_queue_depth)",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "green"
                },
                {
                  "value": 50,
                  "color": "yellow"
                },
                {
                  "value": 100,
                  "color": "red"
                }
              ]
            }
          }
        },
        "options": {
          "displayMode": "gradient",
          "orientation": "horizontal"
        }
      }
    ]
  },
  "overwrite": true
}
